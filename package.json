		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@typescript-eslint/parser": "^7.6.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.37.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"sass": "^1.75.0",
		"svelte": "^4.2.13",
		"svelte-check": "^3.6.9",
		"tslib": "^2.4.1",
